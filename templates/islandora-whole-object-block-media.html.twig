{#
/**
 * @file
 * Template for Islandora Whole Object's Properties block.
 *
 * Available variables:
 * - content: The content generated by the Properties block.
 *
 * @ingroup themeable
 */
#}
  <div class="islandora_whole_object_content">
    <table>
    <th>Media</th><th>Media Use tag(s)</th><th>File URI</th><th>File Mime type</th><th>File last modified</th><th>File hash(es)</th>
    {% for property in content %}
      <tr>
      <td><a href="/media/{{ property.0 }}">{{ property.1 }}</a></td><td>{{ property.2 }}</td><td>{{ property.3 }}</td><td>{{ property.4 }}</td><td>{{ property.5 }}</td><td>{{ property.6 }}</td>
      </tr>
    {% endfor %}
    </table>
  </div>
